<Application x:Class="TgSF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TgSF"
             xmlns:viewModel="clr-namespace:TgSF.MVVM.ModelView"
             xmlns:view="clr-namespace:TgSF.MVVM.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewModel:MainWindowViewModel}">
                <local:MainWindow/>
            </DataTemplate>
            
        </ResourceDictionary>

    </Application.Resources>
</Application>
